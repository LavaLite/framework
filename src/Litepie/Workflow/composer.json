{
    "name": "zerodahero/laravel-workflow",
    "description": "Integerate Symfony Workflow component into Laravel.",
    "keywords": ["workflow", "symfony", "laravel", "laravel5"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "symfony/workflow": "^4.2.9",
        "symfony/process": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "illuminate/console": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "ZeroDaHero\\LaravelWorkflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ZeroDaHero\\LaravelWorkflow\\WorkflowServiceProvider"
            ],
            "aliases": {
                "Workflow": "ZeroDaHero\\LaravelWorkflow\\Facades\\WorkflowFacade"
            }
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.0"
    }
}
