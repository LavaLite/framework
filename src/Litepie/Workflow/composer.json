{
    "name": "litepie/workflow",
    "description": "The Litepie workflow package.",
    "keywords": ["workflow", "laravel", "lavalite"],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1",
        "symfony/workflow": "^6.0",
        "symfony/process": "^6.0",
        "symfony/event-dispatcher-contracts": "^2.4|^3.0",
        "illuminate/console": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Litepie\\Workflow\\": "/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^6.0",
        "fakerphp/faker": "^1.13",
        "orchestra/testbench": "^6.0|^7.0",
        "symfony/contracts": "^2.3"
    }
}
